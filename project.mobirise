        {
          "_name": "header02",
          "_sourceTheme": "authorm5",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"header02 authorm5\" group=\"Headers\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{\n    'mbr-fullscreen': fullScreen,\n    'mbr-parallax-background': bg.parallax}\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\" checked>\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"12\" step=\"1\" value=\"5\" condition=\"fullScreen == false\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"12\" step=\"1\" value=\"5\" condition=\"fullScreen == false\">\n        <input type=\"checkbox\" title=\"Reverse\" name=\"reverseContent\" checked>\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\" checked>\n        <input type=\"checkbox\" title=\"Text\" name=\"showText\" checked>\n        <input type=\"checkbox\" title=\"Buttons\" name=\"showButtons\">\n        <input type=\"color\" title=\"Image Border\" value=\"#373d52\" name=\"border\">\n        <input type=\"color\" title=\"Image Shadow\" value=\"#d2d2df\" name=\"shadow\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"@PROJECT_PATH@/assets/images/the%20new%20ottu%20media%20logo%20(roku)%20-%20made%20with%20postermywall-900x900.png\" parallax selected>\n            <input type=\"color\" title=\"Color\" value=\"#232b45\">\n            <input type=\"video\" title=\"Video\" value=\"https://vimeo.com/428046504\">\n        </fieldset>\n        <header condition=\"bg.type === 'video'\">Fallback Image</header>\n        <input type=\"image\" title=\"Fallback Image\" value=\"@ADDONS_PATH@/authorm5/components/_images/background1.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#232b45\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\" mbr-class=\"{'flex-row-reverse' : reverseContent}\">\n            <div class=\"col-12 col-lg-4\">\n                <div class=\"image-wrapper\">\n                    <img src=\"@PROJECT_PATH@/assets/images/gettyimages-1128104898-7feb60e642bb4a7c97c395744e288be8-788x525.jpg\" alt>\n                </div>\n            </div>\n            <div class=\"col-12 col-lg-8\">\n                <div class=\"content-wrapper\">\n                    <h1 class=\"mbr-section-title mbr-fonts-style\" data-app-selector=\".mbr-section-title, .mbr-section-btn\" mbr-theme-style=\"display-1\" mbr-if=\"showTitle\">Virtual Broadcasting Studios</h1>\n                    <h3 class=\"mbr-section-subtitle mbr-fonts-style\" data-app-selector=\".mbr-section-subtitle\" mbr-theme-style=\"display-5\" mbr-if=\"showSubtitle\">Local Flavor, Global Reach: Unlock the Power of Virtual Broadcasting!</h3>\n                    <p class=\"mbr-text mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".mbr-text\">\n                        Seize this incredible opportunity to elevate your visibility and expand your reach through engaging live radio and television broadcasts. Whether hosting interviews, showcasing products, or delivering thought-provoking discussions - our platform enables you to connect with audiences in ways that resonate deeply. Transform mundane interactions into memorable experiences by harnessing the power of live media broadcasting through our convenient booking system designed for individuals seeking impactful exposure across traditional media channels.\n                    </p>\n                    <div mbr-buttons mbr-theme-style=\"display-4\" class=\"mbr-section-btn\" mbr-if=\"showButtons\" data-toolbar=\"-mbrBtnMove\">\n                        <a class=\"btn btn-primary\" href=\"https://mobiri.se\" data-app-placeholder=\"Type Text\">\n                            VIEW MORE\n                        </a>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
          "_styles": {
            "& when not (@fullScreen)": {
              "padding-top": "(@paddingTop * 1rem)",
              "padding-bottom": "(@paddingBottom * 1rem)"
            },
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)"
            },
            ".mbr-fallback-image.disabled": {
              "display": "none"
            },
            ".mbr-fallback-image": {
              "display": "block",
              "background-size": "cover",
              "background-position": "center center",
              "width": "100%",
              "height": "100%",
              "position": "absolute",
              "top": "0",
              "& when (@bg-type = 'video')": {
                "background-image": "url(@fallBackImage)"
              }
            },
            "& when (@fullWidth)": {
              ".container-fluid": {
                "padding": "0 40px",
                "@media (max-width: 992px)": {
                  "padding": "0 15px"
                }
              }
            },
            ".container": {
              "@media (max-width: 992px)": {
                "padding": "0 15px"
              }
            },
            ".content-wrapper": {
              "padding-left": "80px",
              "& when (@reverseContent)": {
                "padding-left": "0",
                "padding-right": "80px",
                "@media (max-width: 992px)": {
                  "padding": "0"
                }
              },
              "@media (max-width: 992px)": {
                "margin-top": "40px",
                "padding": "0"
              },
              ".mbr-section-subtitle": {
                "margin-bottom": "30px"
              },
              ".mbr-section-title": {
                "margin-bottom": "30px",
                "@media (max-width: 992px)": {
                  "margin-bottom": "20px"
                }
              },
              ".mbr-text": {
                "margin-bottom": "0"
              },
              ".mbr-section-btn": {
                "margin-top": "40px"
              }
            },
            ".image-wrapper": {
              "display": "flex",
              "align-items": "center",
              "justify-content": "center",
              "height": "100%",
              "img": {
                "width": "400px",
                "height": "400px",
                "border": "3px solid @border",
                "box-shadow": "10px 10px 50px @shadow",
                "object-fit": "cover",
                "border-radius": "100% !important",
                "@media (max-width: 1500px)": {
                  "width": "350px",
                  "height": "350px"
                },
                "@media (max-width: 1199px)": {
                  "width": "300px",
                  "height": "300px"
                },
                "@media (max-width: 1200px)": {
                  "width": "260px",
                  "height": "260px"
                }
              }
            },
            ".mbr-section-title": {
              "color": "#ffffff"
            },
            ".mbr-section-subtitle": {
              "color": "#f2ac2b"
            },
            ".mbr-text": {
              "color": "#d2d2df"
            }
          },
