        {
          "_name": "tabs02",
          "_sourceTheme": "essencem5",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"tabs02 essencem5\" group=\"List\" plugins=\"mbr-tabs\" mbr-class=\"{\n    'mbr-fullscreen': fullScreen,\n    'mbr-parallax-background': bg.parallax}\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\" checked>\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"12\" step=\"1\" value=\"5\" condition=\"fullScreen == false\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"12\" step=\"1\" value=\"5\" condition=\"fullScreen == false\">\n        <input type=\"checkbox\" title=\"Reverse\" name=\"reverseContent\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <header>Tabs</header>\n        <select title=\"Count\" name=\"bootstrapTabs\">\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\">4</option>\n            <option value=\"5\">5</option>\n            <option value=\"6\">6</option>\n            <option value=\"7\" selected>7</option>\n            <option value=\"8\">8</option>\n            <option value=\"9\">9</option>\n            <option value=\"10\">10</option>\n        </select>\n        <input type=\"color\" title=\"Tabs Color\" value=\"#ffffff\" name=\"tabs\" selected>\n        <input type=\"color\" title=\"Tabs Active BG\" value=\"#ff6666\" name=\"active\" selected>\n        <input type=\"checkbox\" title=\"Tabs Number\" name=\"showNumber\">\n        <input type=\"checkbox\" title=\"Tabs Icon\" name=\"showIcon\">\n        <input type=\"checkbox\" title=\"Card Text\" name=\"showText\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"@PROJECT_PATH@/assets/images/television%20networks%203-2000x2000.jpg\" selected>\n            <input type=\"color\" title=\"Color\" value=\"#000000\">\n        </fieldset>\n        <header condition=\"bg.type === 'video'\">Fallback Image</header>\n        <input type=\"image\" title=\"Fallback Image\" value=\"@ADDONS_PATH@/essencem5/components/_images/background1.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#000000\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.6\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\" mbr-class=\"{'flex-row-reverse' : reverseContent}\">\n            <div class=\"col-12 col-lg-5\">\n                <div class=\"title-wrapper\">\n                    <h2 class=\"mbr-section-title mbr-fonts-style\" data-app-selector=\".mbr-section-title, .tabs-wrap\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\"><b>TELEVISION NETWORKS</b></h2>\n                </div>\n                <div class=\"tabs-wrap\">\n                    <ul class=\"nav nav-tabs\" role=\"tablist\">\n                        <li mbr-buttons mbr-theme-style=\"display-5\" class=\"nav-item first mbr-fonts-style\" data-toolbar=\"-mbrBtnAdd,-mbrBtnMove,-mbrLink,-mbrBtnRemove,-iconFont\"><a class=\"nav-link show active\" role=\"tab\" data-toggle=\"tab\" data-bs-toggle=\"tab\" href=\"#tabs02-bn_tab0\" aria-selected=\"true\" data-app-placeholder=\"Type Text\">The Christendom Television Network<p class=\"card-number mbr-fonts-style\" data-app-selector=\".card-number\" style>\n                                    01 The Christendom Television Network</p><div class=\"icon-wrapper\">\n                                    <span class=\"mobi-mbri mobi-mbri-up mbr-iconfont\"></span>\n                                </div></a></li>\n                        <li mbr-buttons mbr-theme-style=\"display-5\" class=\"nav-item\" mbr-if=\"bootstrapTabs > 1\" data-toolbar=\"-mbrBtnAdd,-mbrBtnMove,-mbrLink,-mbrBtnRemove,-iconFont\"><a class=\"nav-link mbr-fonts-style active\" role=\"tab\" data-toggle=\"tab\" data-bs-toggle=\"tab\" href=\"#tabs02-bn_tab1\" data-app-placeholder=\"Type Text\" aria-selected=\"true\">God, Family &amp; Country Television Network<div class=\"icon-wrapper\" style>\n                                    <span class=\"mobi-mbri mobi-mbri-up mbr-iconfont\"></span>\n                                </div></a></li>\n                        <li mbr-buttons mbr-theme-style=\"display-5\" class=\"nav-item\" mbr-if=\"bootstrapTabs > 2\" data-toolbar=\"-mbrBtnAdd,-mbrBtnMove,-mbrLink,-mbrBtnRemove,-iconFont\"><a class=\"nav-link mbr-fonts-style active\" role=\"tab\" data-toggle=\"tab\" data-bs-toggle=\"tab\" href=\"#tabs02-bn_tab2\" data-app-placeholder=\"Type Text\" aria-selected=\"true\">The REEL Television Network<div class=\"icon-wrapper\" style>\n                                    <span class=\"mobi-mbri mobi-mbri-up mbr-iconfont\"></span>\n                                </div></a></li>\n                        <li mbr-buttons mbr-theme-style=\"display-5\" class=\"nav-item\" mbr-if=\"bootstrapTabs > 3\" data-toolbar=\"-mbrBtnAdd,-mbrBtnMove,-mbrLink,-mbrBtnRemove,-iconFont\"><a class=\"nav-link mbr-fonts-style active\" role=\"tab\" data-toggle=\"tab\" data-bs-toggle=\"tab\" href=\"#tabs02-bn_tab3\" data-app-placeholder=\"Type Text\" aria-selected=\"true\">The Valuable Television Network<div class=\"icon-wrapper\" style>\n                                    <span class=\"mobi-mbri mobi-mbri-up mbr-iconfont\"></span>\n                                </div></a></li>\n                        <li mbr-buttons mbr-theme-style=\"display-5\" class=\"nav-item\" mbr-if=\"bootstrapTabs > 4\" data-toolbar=\"-mbrBtnAdd,-mbrBtnMove,-mbrLink,-mbrBtnRemove,-iconFont\"><a class=\"nav-link mbr-fonts-style active\" role=\"tab\" data-toggle=\"tab\" data-bs-toggle=\"tab\" href=\"#tabs02-bn_tab4\" data-app-placeholder=\"Type Text\" aria-selected=\"true\">Urban Core Television Network<div class=\"icon-wrapper\" style>\n                                    <span class=\"mobi-mbri mobi-mbri-up mbr-iconfont\"></span>\n                                </div></a></li>\n                        <li mbr-buttons mbr-theme-style=\"display-5\" class=\"nav-item\" mbr-if=\"bootstrapTabs > 5\" data-toolbar=\"-mbrBtnAdd,-mbrBtnMove,-mbrLink,-mbrBtnRemove,-iconFont\"><a class=\"nav-link mbr-fonts-style active\" role=\"tab\" data-toggle=\"tab\" data-bs-toggle=\"tab\" href=\"#tabs02-bn_tab5\" data-app-placeholder=\"Type Text\" aria-selected=\"true\">New Wind International Television Network<div class=\"icon-wrapper\" style>\n                                    <span class=\"mobi-mbri mobi-mbri-up mbr-iconfont\"></span>\n                                </div></a></li>\n                        <li mbr-buttons mbr-theme-style=\"display-5\" class=\"nav-item\" mbr-if=\"bootstrapTabs > 6\" data-toolbar=\"-mbrBtnAdd,-mbrBtnMove,-mbrLink,-mbrBtnRemove,-iconFont\"><a class=\"nav-link mbr-fonts-style active\" role=\"tab\" data-toggle=\"tab\" data-bs-toggle=\"tab\" href=\"#tabs02-bn_tab6\" data-app-placeholder=\"Type Text\" aria-selected=\"true\">NEW (Non-Stop Entertainment Worldwide) Streaming Network</a></li>\n                        <li mbr-buttons mbr-theme-style=\"display-5\" class=\"nav-item\" mbr-if=\"bootstrapTabs > 7\" data-toolbar=\"-mbrBtnAdd,-mbrBtnMove,-mbrLink,-mbrBtnRemove,-iconFont\">\n                            <a class=\"nav-link mbr-fonts-style\" role=\"tab\" data-toggle=\"tab\" data-bs-toggle=\"tab\" href=\"#tab8\" data-app-placeholder=\"Type Text\" aria-selected=\"true\">\n                                <p class=\"card-number mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".card-number\" mbr-if=\"showNumber\">\n                                    08\n                                </p>\n                                Furniture\n                                <div class=\"icon-wrapper\" mbr-if=\"showIcon\">\n                                    <span mbr-icon class=\"mobi-mbri mobi-mbri-up mbr-iconfont\"></span>\n                                </div>\n                            </a>\n                        </li>\n                        <li mbr-buttons mbr-theme-style=\"display-5\" class=\"nav-item\" mbr-if=\"bootstrapTabs > 8\" data-toolbar=\"-mbrBtnAdd,-mbrBtnMove,-mbrLink,-mbrBtnRemove,-iconFont\">\n                            <a class=\"nav-link mbr-fonts-style\" role=\"tab\" data-toggle=\"tab\" data-bs-toggle=\"tab\" href=\"#tab9\" data-app-placeholder=\"Type Text\" aria-selected=\"true\">\n                                <p class=\"card-number mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".card-number\" mbr-if=\"showNumber\">\n                                    09\n                                </p>\n                                Printing\n                                <div class=\"icon-wrapper\" mbr-if=\"showIcon\">\n                                    <span mbr-icon class=\"mobi-mbri mobi-mbri-up mbr-iconfont\"></span>\n                                </div>\n                            </a>\n                        </li>\n                        <li mbr-buttons mbr-theme-style=\"display-5\" class=\"nav-item\" mbr-if=\"bootstrapTabs > 9\" data-toolbar=\"-mbrBtnAdd,-mbrBtnMove,-mbrLink,-mbrBtnRemove,-iconFont\">\n                            <a class=\"nav-link mbr-fonts-style\" role=\"tab\" data-toggle=\"tab\" data-bs-toggle=\"tab\" href=\"#tab10\" data-app-placeholder=\"Type Text\" aria-selected=\"true\">\n                                <p class=\"card-number mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".card-number\" mbr-if=\"showNumber\">\n                                    10\n                                </p>\n                                Beauty\n                                <div class=\"icon-wrapper\" mbr-if=\"showIcon\">\n                                    <span mbr-icon class=\"mobi-mbri mobi-mbri-up mbr-iconfont\"></span>\n                                </div>\n                            </a>\n                        </li>\n                    </ul>\n                </div>\n            </div>\n            <div class=\"col-12 col-lg-6\">\n                <div class=\"tab-content\">\n                    <div id=\"tab1\" class=\"tab-pane in active\" role=\"tabpanel\">\n                        <div class=\"content-wrapper\">\n                            <p class=\"card-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card-text\" mbr-if=\"showText\">\n                                The packaging industry benefits from recycled materials, especially recycled paper and\n                                cardboard, recycled plastics, and recycled glass. Many companies are adopting\n                                eco-friendly packaging to reduce waste and minimize their environmental footprint.\n                            </p>\n                            <div class=\"image-wrapper\">\n                                <a href=\"ChristendomTV.html\"><img src=\"@PROJECT_PATH@/assets/images/the%20christendom%20television%20-%20made%20with%20postermywall-900x900.jpg\" alt></a>\n                            </div>\n                        </div>\n                    </div>\n                    <div id=\"tab2\" class=\"tab-pane\" role=\"tabpanel\" mbr-if=\"bootstrapTabs > 1\">\n                        <div class=\"content-wrapper\">\n                            <p class=\"card-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card-text\" mbr-if=\"showText\">\n                                The fashion industry is incorporating recycled textiles into clothing and accessories.\n                                From recycled cotton and polyester to upcycled materials, sustainable fashion practices\n                                are gaining traction, addressing concerns about the environmental impact\n                            </p>\n                            <div class=\"image-wrapper\">\n                                <a href=\"GFCTV.html\"><img src=\"@PROJECT_PATH@/assets/images/gfc%20logo%20-%20made%20with%20postermywall-900x900.jpg\" alt></a>\n                            </div>\n                        </div>\n                    </div>\n                    <div id=\"tab3\" class=\"tab-pane\" role=\"tabpanel\" mbr-if=\"bootstrapTabs > 2\">\n                        <div class=\"content-wrapper\">\n                            <p class=\"card-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card-text\" mbr-if=\"showText\">\n                                The construction industry utilizes recycled materials such as recycled concrete,\n                                recycled steel, and reclaimed wood. Using these materials helps reduce the demand for\n                                virgin resources and minimizes the environmental impact of construction projects.\n                            </p>\n                            <div class=\"image-wrapper\">\n                                <a href=\"REELTV.html\"><img src=\"@PROJECT_PATH@/assets/images/the%20reel%20tv%20network%20logo%202%20-%20made%20with%20postermywall-900x900.jpg\" alt></a>\n                            </div>\n                        </div>\n                    </div>\n                    <div id=\"tab4\" class=\"tab-pane\" role=\"tabpanel\" mbr-if=\"bootstrapTabs > 3\">\n                        <div class=\"content-wrapper\">\n                            <p class=\"card-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card-text\" mbr-if=\"showText\">\n                                The automotive industry incorporates recycled materials into vehicle components.\n                                Recycled plastics, metals, and rubber can be found in various parts of vehicles,\n                                contributing to sustainability efforts within the sector.\n                            </p>\n                            <div class=\"image-wrapper\">\n                                <a href=\"VTN.html\"><img src=\"@PROJECT_PATH@/assets/images/original%20logo-1256x1288.png\" alt></a>\n                            </div>\n                        </div>\n                    </div>\n                    <div id=\"tab5\" class=\"tab-pane\" role=\"tabpanel\" mbr-if=\"bootstrapTabs > 4\">\n                        <div class=\"content-wrapper\">\n                            <p class=\"card-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card-text\" mbr-if=\"showText\">\n                                The electronics industry is increasingly recognizing the importance of recycling and\n                                incorporating recycled materials into electronic devices. Recycled plastics, metals, and\n                                other components contribute to reducing electronic waste and promoting a circular\n                                economy.\n                            </p>\n                            <div class=\"image-wrapper\">\n                                <a href=\"UCTN.html\"><img src=\"@PROJECT_PATH@/assets/images/urban%20core%20television%20network-400x400.png\" alt></a>\n                            </div>\n                        </div>\n                    </div>\n                    <div id=\"tab6\" class=\"tab-pane\" role=\"tabpanel\" mbr-if=\"bootstrapTabs > 5\">\n                        <div class=\"content-wrapper\">\n                            <p class=\"card-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card-text\" mbr-if=\"showText\">\n                                Recycled materials play a role in the packaging of food and beverages. Recycled\n                                plastics, paper, and glass are commonly used in packaging, providing sustainable\n                                alternatives to traditional packaging materials.\n                            </p>\n                            <div class=\"image-wrapper\">\n                                <a href=\"https://www.newwindstl.org/newwindstltvnetwork/\" target=\"_blank\"><img src=\"@PROJECT_PATH@/assets/images/img_20181006_074319-678-1186x1072.jpg\" alt></a>\n                            </div>\n                        </div>\n                    </div>\n                    <div id=\"tab7\" class=\"tab-pane\" role=\"tabpanel\" mbr-if=\"bootstrapTabs > 6\">\n                        <div class=\"content-wrapper\">\n                            <p class=\"card-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card-text\" mbr-if=\"showText\">\n                                Retailers are embracing recycled goods in their product offerings, from clothing to home\n                                goods. Consumers are increasingly seeking eco-friendly options, prompting many retailers\n                                to incorporate recycled materials into their merchandise.\n                            </p>\n                            <div class=\"image-wrapper\">\n                                <a href=\"https://tvsw5-hls.secdn.net/tvsw5-chorigin/play/prod-3876da92893d4d6092e19faede892f3b/playlist.m3u8\" target=\"_blank\"><img src=\"@PROJECT_PATH@/assets/images/1%20new%20streaming%20network%20logo%20-500x500.png\" alt></a>\n                            </div>\n                        </div>\n                    </div>\n                    <div id=\"tab8\" class=\"tab-pane\" role=\"tabpanel\" mbr-if=\"bootstrapTabs > 7\">\n                        <div class=\"content-wrapper\">\n                            <p class=\"card-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card-text\" mbr-if=\"showText\">\n                                The furniture industry benefits from the use of recycled wood, metal, and plastics.\n                                Companies are designing and manufacturing furniture using reclaimed and recycled\n                                materials to create sustainable and environmentally friendly products.\n                            </p>\n                            <div class=\"image-wrapper\">\n                                <img src=\"@ADDONS_PATH@/essencem5/components/_images/essence/image4.jpg\" alt=\"Mobirise\">\n                            </div>\n                        </div>\n                    </div>\n                    <div id=\"tab9\" class=\"tab-pane\" role=\"tabpanel\" mbr-if=\"bootstrapTabs > 8\">\n                        <div class=\"content-wrapper\">\n                            <p class=\"card-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card-text\" mbr-if=\"showText\">\n                                The paper and printing industry relies on recycled paper to produce a variety of\n                                products, including newspapers, packaging materials, and office supplies. Using recycled\n                                paper helps conserve forests and reduce energy consumption.\n                            </p>\n                            <div class=\"image-wrapper\">\n                                <img src=\"@ADDONS_PATH@/essencem5/components/_images/essence/image11.jpg\" alt=\"Mobirise\">\n                            </div>\n                        </div>\n                    </div>\n                    <div id=\"tab10\" class=\"tab-pane\" role=\"tabpanel\" mbr-if=\"bootstrapTabs > 9\">\n                        <div class=\"content-wrapper\">\n                            <p class=\"card-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card-text\" mbr-if=\"showText\">\n                                The beauty and personal care industry incorporate recycled materials into packaging and\n                                product formulations. Sustainable practices, such as using recycled plastics and\n                                minimizing packaging waste, are becoming integral to brand strategies.\n                            </p>\n                            <div class=\"image-wrapper\">\n                                <img src=\"@ADDONS_PATH@/essencem5/components/_images/essence/image15.jpg\" alt=\"Mobirise\">\n                            </div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
          "_styles": {
            "& when not (@fullScreen)": {
              "padding-top": "(@paddingTop * 1rem)",
              "padding-bottom": "(@paddingBottom * 1rem)"
            },
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "overflow": "hidden",
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)"
            },
            ".mbr-fallback-image.disabled": {
              "display": "none"
            },
            ".mbr-fallback-image": {
              "display": "block",
              "background-size": "cover",
              "background-position": "center center",
              "width": "100%",
              "height": "100%",
              "position": "absolute",
              "top": "0",
              "& when (@bg-type = 'video')": {
                "background-image": "url(@fallBackImage)"
              }
            },
            "& when (@fullWidth)": {
              ".container-fluid": {
                "padding": "0 80px",
                "@media (max-width: 1200px)": {
                  "padding": "0 40px"
                },
                "@media (max-width: 992px)": {
                  "padding": "0 22px"
                }
              }
            },
            ".container": {
              "@media (max-width: 992px)": {
                "padding": "0 22px"
              }
            },
            ".row": {
              "justify-content": "space-between"
            },
            ".title-wrapper": {
              ".mbr-section-title": {
                "margin-bottom": "20px"
              }
            },
            ".tabs-wrap": {
              ".nav.nav-tabs": {
                "display": "inline-block",
                "width": "fit-content",
                "border": "none",
                "@media (max-width: 992px)": {
                  "display": "block",
                  "width": "100%"
                },
                ".nav-item": {
                  "padding": "0",
                  "border": "none",
                  "border-radius": "0",
                  "margin": "0",
                  ".nav-link": {
                    "display": "flex",
                    "align-items": "center",
                    "padding": "15px 80px 15px 20px",
                    "margin-bottom": "0",
                    "border": "none",
                    "border-bottom": "1px solid @tabs",
                    "color": "@tabs",
                    "background-color": "transparent",
                    "border-radius": "0 !important",
                    "transition": "all 0.3s ease-in-out",
                    "@media (max-width: 1200px)": {
                      "padding": "15px 20px"
                    },
                    "@media (max-width: 768px)": {
                      "padding": "15px 0"
